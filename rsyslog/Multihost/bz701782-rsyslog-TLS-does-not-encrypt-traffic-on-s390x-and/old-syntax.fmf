    continue: false
-   enabled: false
    when: distro ~< rhel-5.8
    continue: false
-   enabled: false
    when: distro ~< rhel-6.3
    continue: false
extra-nitrate: TC#0098170
extra-summary: rsyslogSyntax=old /CoreOS/rsyslog/Multihost/bz701782-rsyslog-TLS-does-not-encrypt-traffic-on-s390x-and
